{
  "@context" : {
    "mass": "https://auroral.iot.linkeddata.es/def/biomass#",
    "dc": "http://purl.org/dc/terms",
    "geo": "http://www.w3.org/2003/01/geo/wgs84_pos",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "name" : {
      "@id": "mass:hasName",
      "@type": "xsd:string"
    },
    "uuid" : {
      "@id": "mass:hasID",
      "@type": "xsd:string"
    },
    "biomass_origin" : {
      "@id": "mass:hasOrigin",
      "@type": "@id",
      "@context" : {
         "uuid" : {
            "@id": "mass:hasID",
            "@type": "xsd:string"
          },
         "description" : {
            "@id": "mass:hasDescription",
            "@type": "xsd:string"
          },
          "group" : {
              "@id": "mass:hasGroup",
              "@type": "@id",
              "@context" : {
                    "group_id": { "@id": "mass:hasID", "@type": "xsd:string" },
                    "group_name": { "@id": "mass:hasName", "@type": "xsd:string" },
                    "group_description": { "@id": "mass:hasDescription", "@type": "xsd:string" }
              }
          },
          "subgroup" : {
              "@id": "mass:hasSubGroup",
              "@type": "@id",
              "@context" : {
                    "group_id": { "@id": "mass:hasID", "@type": "xsd:string" },
                    "group_name": { "@id": "mass:hasName", "@type": "xsd:string" },
                    "group_description": { "@id": "mass:hasDescription", "@type": "xsd:string" }
              }
          },
        "type" : {
            "@id"  : "mass:belongsToType",
          	"@type": "@vocab"
        },
        "broad_leaf" : "mass:BroadLeaf",
        "coniferous" : "mass:Coniferous",
        "bushes" : "mass:Bushes",
        "bark" : "mass:Bark",
        "seeds" : "mass:Seeds"
      }
    }
  }
}
